<%= render 'partials/meny' %>
<article id="profile" class="col-md-10 col-md-offset-2">
  <!--<img src="#"/>-->
  <h1>Profile for <%= current_user.email %></h1>
  <h1>@use<%= @user.username %></h1>

    <div id="main">

      <% form_for @user do |f| %>
        <% f.text_field :username %>
        <% f.submit 'Set username' %>
      <% end %>

    </div>
  <%= render 'partials/footer' %>
</article>
